

page {
  padding: 0 40rpx;
}

.select {
  margin-top: 20rpx;
}

.select-item {
  margin-right: 28rpx;
  font-size: 28rpx;
}

.select-item_at {
  padding: 4rpx;
  border-bottom: 4rpx solid #5a7f5b;
  color: #5a7f5b;
  font-size: 36rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
}
.win{
  margin-top: 40rpx; 
  border-radius: 40rpx;
  width: 100%;
  box-shadow: 0 1rpx 1rpx 1rpx #d0d0d0;
  overflow: hidden;
}
.win-map{
  width: 100%;
  height: 300rpx;
}
.win-bottom{
  position: relative;
  padding: 74rpx 32rpx 32rpx 56rpx;
  width: 100%;
}

.win-bar{
  position: absolute;
  top: -50rpx;
  left: 30rpx;
  padding: 14rpx 30rpx;
  background: #5A805C;
  border: 4rpx solid #fff;
  border-radius: 0 40rpx 40rpx 40rpx;
  max-width: 554rpx;
  color: #fff;
  font-size: 28rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  box-shadow: 0 1rpx 1rpx 1rpx #f9f9f9;
}
.win-list{
  font-size: 24rpx;
  color: #D0D0D0;
}
.win-list_see{
  margin-left: 30rpx;
  color:rgba(0,0,0,0.18);
}

.noopen {
  font-size: 28rpx;
  color: rgba(0, 0, 0, 0.18);
  margin-top: 384rpx;
  text-align: center;
}
