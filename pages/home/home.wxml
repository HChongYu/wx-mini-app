<view class="jh-listnp search-back" bindtap='searchClick'>
  <view class="jh-list_item search">快来一搜</view>
  <view class="search-icon"></view>
</view>
<view class="jh-listnp selbar">
  <view class="selbar-item" wx:for="{{iconBar}}" wx:key='index' bindtap="iconClik">
    <image class="selbar-item-icon" src="{{item.icon}}"></image>
    <view>{{item.name}}</view>
  </view>
</view>
<view class='article-bar'></view>
<jh-navbar in-style='margin-top:60rpx;' navbar-data='{{navbarData}}' bind:callBack='homeBarBack'></jh-navbar>
<view wx:if='{{atIndex==0}}'>
  <view wx:for='{{dome1}}' wx:key="id" style='padding-top: 40rpx;{{index==0? "":"  border-top: 2rpx solid #F9F9F9;"}}'>
    <jh-win win-data='{{item}}' bind:callBack='homeWinBack'></jh-win>
  </view>
</view>
<view wx:elif='{{atIndex==1}}'>
  <view wx:for='{{dome2}}' wx:key="id" style='padding-top: 40rpx;{{index==0? "":"  border-top: 2rpx solid #F9F9F9;"}}'>
    <jh-win win-data='{{item}}' bind:callBack='homeWinBack'></jh-win>
  </view>
</view>
<view class='jh-bottom_hint'>我是有底线的</view>