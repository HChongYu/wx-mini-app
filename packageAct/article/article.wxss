@import "/wxParse/wxParse.wxss";

.collect-btn_already {
  margin: auto;
  width:218rpx;
  height:60rpx;
  line-height: 60rpx;
  border-radius:48rpx;
  font-size: 24rpx;
  text-align: center;
  color:#fff;
  background: #2DA25F;
}

.collect-btn_not {
