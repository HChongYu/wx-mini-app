.win {
  margin-top: 40rpx;
  border-radius: 40rpx;
  width: 100%;
  box-shadow: 0 1rpx 1rpx 1rpx #d0d0d0;
  overflow: hidden;
}

.win-map {
  width: 100%;
  height: 400rpx;
}

.win-bottom {
  padding: 24rpx 24rpx 28rpx 40rpx;
  width: 100%;
}

.win-list {
  margin-bottom: 4rpx;
}

.win-list_main {
  font-size: 32rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
}

.win-list_time {
  font-size: 24rpx;
  color: #d0d0d0;
}

.win-list_minor {
  font-size: 28rpx;
  color:rgba(0,0,0,.46);
}
