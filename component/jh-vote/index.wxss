/* pagekagelist/pages/y-phoneup/index.wxss */

.jh-vote_back {
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.8);
  height: 100vh;
  width: 100%;
  z-index: 99999;
}

.jh-vote {
  position: absolute;
  left: 0;
  bottom: 0;
  padding: 24rpx;
  margin-bottom: 100rpx;
}

.jh-vote-hint {
  padding: 20rpx 32rpx;
  color: #fff;
  font-size: 28rpx;
}

.jh-vote-header {
  padding: 28rpx 32rpx;
  border-radius: 24rpx;
  background-color: #fff;
}

.jh-vote-header-title {
  margin-top: 4rpx;
  font-size: 28rpx;
  color: #2da25f;
}

.jh-vote-header-text {
  margin-top: 16rpx;
  font-size: 24rpx;
  color: #333;
  font-family: PingFangSC-Medium;
  font-weight: 500;
}

.jh-vote-item {
  position: relative;
  left: 238rpx;
  margin-top: 20rpx; 
  width: 464rpx;
  height: 74rpx;
  line-height: 74rpx;
  background:#7CD19F;
  border-radius:37rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: #fff;
  text-align: center;
  
 
}
