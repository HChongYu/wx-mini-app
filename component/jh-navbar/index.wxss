/* component/jh-navbar/indedx.wxss */
@import "/wxss/overall.wxss";

.navbar {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  margin-top: 20rpx;
}

.navbar-item {
  display: inline-block;
  margin-left: 14rpx;
  margin-right: 14rpx;
  font-size: 28rpx;
}

.navbar-item_at {
  padding: 4rpx;
  border-bottom: 4rpx solid #2DA25F;
  color: #2DA25F;
  font-size: 36rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
}
